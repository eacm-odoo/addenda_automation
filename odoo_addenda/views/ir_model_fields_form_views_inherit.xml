        <field name="model">ir.model.fields</field>
        <field name="inherit_id" ref="base.view_model_fields_form" />
        <field name="arch" type="xml">
            <field name="readonly" position="after">
                <field name="store" />
                <field name="index" />
                <field name="copied" />
            </field>
            <field name="tracking" position="attributes">
